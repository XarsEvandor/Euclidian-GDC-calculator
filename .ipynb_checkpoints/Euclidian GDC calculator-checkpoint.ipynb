   "source": [
    "import math\n",
    "\n",
    "x = 17\n",
    "y = 2\n",
    "conf = math.gcd(x,y)\n",
    "\n",
    "while y != 0:\n",
    "    print(str(x) + \" = \" + str(y) + \" * \" + str(x//y) + \" + \" + str(x%y))\n",
    "    r = x%y\n",
    "    x = y\n",
    "    y = r\n",
    "\n",
    "print(\"GCD --> \" + str(x))\n",
    "print(\"Confirmation --> \" + str(conf))"
   ]
  }
